<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CircleActivity">

    <TextView
        android:id="@+id/Tcenter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="20dp"
        android:text="sdfgsdfsdfsd"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="20dp"
        android:text="sdfgsdfsdfsd"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/img_center"
        style="@style/Circle_image"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/circle_style" />

    <ImageView
        android:id="@+id/imageView2"
        style="@style/Circle_image"
        app:layout_constraintBottom_toTopOf="@id/img_center"
        app:layout_constraintCircle="@id/img_center"
        app:layout_constraintCircleAngle="0"
        app:layout_constraintCircleRadius="150dp"
        app:layout_constraintStart_toStartOf="@id/img_center"
        app:srcCompat="@drawable/circle_style" />

    <ImageView
        android:id="@+id/imageView3"
        style="@style/Circle_image"
        app:layout_constraintBottom_toTopOf="@id/img_center"
        app:layout_constraintCircle="@id/img_center"
        app:layout_constraintCircleAngle="60"
        app:layout_constraintCircleRadius="150dp"
        app:layout_constraintStart_toEndOf="@id/img_center"
        app:srcCompat="@drawable/circle_style" />

    <ImageView
        android:id="@+id/imageView4"
        style="@style/Circle_image"
        app:layout_constraintCircle="@id/img_center"
        app:layout_constraintCircleAngle="120"
        app:layout_constraintCircleRadius="150dp"
        app:layout_constraintStart_toEndOf="@id/img_center"
        app:layout_constraintTop_toBottomOf="@id/img_center"
        app:srcCompat="@drawable/circle_style" />


    <ImageView
        android:id="@+id/imageView5"
        style="@style/Circle_image"
        app:layout_constraintCircle="@id/img_center"
        app:layout_constraintCircleAngle="180"
        app:layout_constraintCircleRadius="150dp"
        app:layout_constraintStart_toStartOf="@id/img_center"
        app:layout_constraintTop_toBottomOf="@id/img_center"
        app:srcCompat="@drawable/circle_style" />

    <ImageView
        android:id="@+id/imageView6"
        style="@style/Circle_image"
        app:layout_constraintCircle="@id/img_center"
        app:layout_constraintCircleAngle="240"
        app:layout_constraintCircleRadius="150dp"
        app:layout_constraintEnd_toStartOf="@id/img_center"
        app:layout_constraintTop_toBottomOf="@id/img_center"
        app:srcCompat="@drawable/circle_style" />

    <ImageView
        android:id="@+id/imageView7"
        style="@style/Circle_image"
        app:layout_constraintBottom_toTopOf="@id/img_center"
        app:layout_constraintCircle="@id/img_center"
        app:layout_constraintCircleAngle="300"
        app:layout_constraintCircleRadius="150dp"
        app:layout_constraintEnd_toStartOf="@id/img_center"
        app:srcCompat="@drawable/circle_style" />


</androidx.constraintlayout.widget.ConstraintLayout>